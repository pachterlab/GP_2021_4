nohup $SCATAK_HOME/scATAK -module=quant -id=sample_sheet.csv -wlist=737K-cratac-v1.txt \
       -genome=/home/fgao/reference/Homo_sapiens.GRCh38.dna_rm.primary_assembly.fa \
       -gene=/home/fgao/reference/Homo_sapiens.GRCh38.101.chr.gtf \
       -bc=16 -bf=40 -t=16 > nohup_quant.txt 2>&1 &

nohup $SCATAK_HOME/scATAK -module=track -bg=bc_group.txt -bam=peak_calling/HIP1.bam -genome=/home/fgao/reference/Homo_sapiens.GRCh38.dna_rm.primary_assembly.fa > nohup_track.txt 2>&1 &

nohup $SCATAK_HOME/scATAK -module=hic -bg=bc_group.txt -hic=HIP1_interactions.bedpe -bin=10000 -mtxdir=atac_regions/atac_HIP1 -t=80 > nohup_hic.txt 2>&1 &
